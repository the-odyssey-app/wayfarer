{
  "version": 2,
  "builds": [
    {
      "src": "api/places-search.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/openrouter.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/places-search",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "api/places-search.js"
    },
    {
      "src": "/api/openrouter",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "api/openrouter.js"
    }
  ]
}
